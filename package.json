{
  "name": "music-player",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:scss": "npx stylelint ./src/**/*.scss",
    "lint:ts": "eslint ./src --ext .ts,.tsx --fix --quiet",
    "lint:staged": "./node_modules/.bin/lint-staged",
    "prettify:check": "npx prettier --check .",
    "prettify:write": "npx prettier --write ."
  },
  "dependencies": {
    "antd": "^5.1.7",
    "axios": "^1.3.2",
    "classnames": "^2.3.2",
    "next": "13.1.6",
    "next-plugin-antd-less": "^1.8.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@next/font": "13.1.6",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "babel-plugin-import": "^1.13.5",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "sass": "^1.58.0",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,scss}": [
      "yarn prettify:write"
    ],
    "*.{ts,tsx}": [
      "yarn lint:ts"
    ],
    "*.{css,scss}": [
      "yarn lint:scss"
    ]
  }
}
